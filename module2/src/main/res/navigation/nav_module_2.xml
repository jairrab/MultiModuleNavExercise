<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_module_2"
    app:startDestination="@id/module_2_fragment_a">

    <fragment
        android:id="@+id/module_2_fragment_a"
        android:name="com.github.jairrab.module2.Module2FragmentA"
        android:label="StateFlowLauncher"
        tools:layout="@layout/module_2_fragment_a">
        <action
            android:id="@+id/action_module_2_fragment_a_to_module_2_fragment_b"
            app:destination="@id/module_2_fragment_b" />
        <deepLink
            android:id="@+id/deepLink"
            app:uri="myapp://module2" />
    </fragment>

    <fragment
        android:id="@+id/module_2_fragment_b"
        android:name="com.github.jairrab.module2.Module2FragmentB"
        android:label="StateFlowLauncher"
        tools:layout="@layout/module_2_fragment_a">
        <action
            android:id="@+id/action_module_2_fragment_b_to_module_2_fragment_c"
            app:destination="@id/module_2_fragment_c" />
    </fragment>

    <fragment
        android:id="@+id/module_2_fragment_c"
        android:name="com.github.jairrab.module2.Module2FragmentC"
        android:label="StateFlowLauncher"
        tools:layout="@layout/module_2_fragment_c" />

</navigation>